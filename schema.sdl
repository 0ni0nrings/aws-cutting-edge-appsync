type Query {
  getVotingResults(nextToken: String): VotingResults
  getServices(nextToken: String): Services
}

type Mutation {
  vote(service: String!): Boolean
}

schema {
  query: Query
  mutation: Mutation
}

type Services {
  items: [String]
  nextToken: String
}

type VotingResults {
  items: [VotingResult]
  nextToken: String
}

type VotingResult {
  service: String!
  upvotes: Int!
}
